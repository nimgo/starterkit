{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "moduleResolution": "node",
    "baseUrl": "src",
    "noImplicitAny": false,
    "sourceMap": true,
    "outDir": "build/distro",
    "strictNullChecks": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react"
  },
  "exclude": [
    "node_modules"
  ]
}